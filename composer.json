{
    "name": "wshafer/assetmanager",
    "description": "An assetmanager module for Zend Framework.",
    "license": "BSD-2-Clause",
    "type": "library",
    "keywords": [
        "asset",
        "asset manager",
        "zf3",
        "zend framework"
    ],
    "homepage": "https://github.com/wshafer/AssetManager/wiki",
    "authors": [
        {
            "name": "Wesley Overdijk",
            "email": "r.w.overdijk@gmail.com",
            "homepage": "http://blog.spoonx.nl/"
        },
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        },
        {
            "name": "Westin Shafer",
            "email": "shafer_w2002@yahoo.com",
            "homepage": "https://github.com/wshafer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-modulemanager": "^2.8.0",
        "wshafer/assetmanager-core": "^1.0.0",
        "zendframework/zend-psr7bridge": "^0.2.2",
        "zendframework/zend-mvc": "^3.1.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.7.0"
    },
    "require-dev": {
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-mvc-console": "^1.1",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "suggest": {
        "zendframework/zend-mvc-console": "zend-mvc-console provides the ability to expose zend-mvc as a console application"
    },
    "autoload": {
        "psr-4": {
            "AssetManager\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "module": "AssetManager"
        }
    },
    "provide": {
        "rwoverdijk/assetmanager" : "2.0.0"
    },
    "autoload-dev": {
        "psr-4": {
            "AssetManager\\Test\\": "tests/"
        }
    }
}
